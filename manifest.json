{
    "manifest_version": 2,
    "name": "CSS Debugger",
    "version": "1.0",
    "description": "Inspect CSS files on the current page",
    "permissions": ["activeTab", "storage"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  